{
  "name": "url-shortener",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:compile": "remix vite:build",
    "build:database": "prisma migrate dev --name init",
    "build": "npm-run-all build:compile build:database",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production bun run server.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@remix-run/express": "^2.10.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@sentry/remix": "^8.17.0",
    "@sentry/vite-plugin": "^2.21.1",
    "@types/express": "^4.17.21",
    "bun": "^1.1.20",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "install": "^0.13.0",
    "isbot": "^5.0.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "npm-run-all2": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.7.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.38",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}